{
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/^repo-tracker\\.ya?ml$/"
      ],
      "matchStrings": [
        "- github\\.com\\/(?<depName>\\S+)\\s*(?<currentDigest>[a-f0-9]{40})\\s#(?<currentValue>\\S+)"
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "{{depName}}",
      "versioningTemplate": "semver"
    }
  ]
}
